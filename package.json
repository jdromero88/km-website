{
  "name": "Kelley-Matney",
  "version": "1.0.0",
  "description": "Kelley Matney website",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "css-build": "sass src/assets/_scss/styles.scss src/css/main.css",
    "css-watch": "npm run css-build -- --watch",
    "css-postcss": "postcss --use autoprefixer --output src/css/index.css src/css/index.css",
    "css-nano": "postcss src/css/main.css > src/css/index.css",
    "build": "webpack",
    "start-webpack":"webpack serve --open",
    "start": "npm-run-all --parallel css-watch start-webpack",
    "start:old": "npm run css-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdromero88/base-web-html.git"
  },
  "author": "Jose Romero",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/jdromero88/base-web-html/issues"
  },
  "homepage": "https://github.com/jdromero88/base-web-html#readme",
  "devDependencies": {
    "autoprefixer": "^10.3.1",
    "css-loader": "^6.2.0",
    "cssnano": "^5.0.7",
    "html-webpack-plugin": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "style-loader": "^3.2.1",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
